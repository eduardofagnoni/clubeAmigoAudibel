        <script>

            /* --------------------------------------------------------------------
            //desabilita scrollme no mobile
            ---------------------------------------------------------------------*/
            var s = scrollme;
            $(document).on( s.init_events.join(  ' '  ), function() { s.init("disable_on_mobile") } );

            /* --------------------------------------------------------------------
            //desabilita scrollme no mobile
            ---------------------------------------------------------------------*/  

            $(document).ready(function(){

                $(".scroll a").click(function(event){
                    event.preventDefault();
                    $('html,body').animate({scrollTop:$(this.hash).offset().top}, 800);
                });

                $("#pagina").smoothWheel()

                $('.bxslider-banner').bxSlider({
                    pager: true,
                    auto: true,
                    controls: false,
                    pause: 9000,
                    stopAutoOnClick: true,
                    infiniteLoop: true,
                }); 

            });




            /* --------------------------------------------------------------------------------
               Círculo contador
               o valor da variável 'value' é o total de dias
               no trecho 'stepValue.toFixed(2)' o número '2' é
               a quantidade de casa que aparece no contador
               se faltar mais de 99 dias o número deve ser incrementado
               para '3' sendo menor ou igual a 99 permanece com '2'
            -------------------------------------------------------------------------------- */
            (function($) {

              $('.third.circle').circleProgress({
                value: 0.55,
                fill: {gradient: [['#bf0811', .5], ['#ff0000', .5]], gradientAngle: Math.PI / 4}
              }).on('circle-animation-progress', function(event, progress, stepValue) {
                $(this).find('strong').text(stepValue.toFixed(2).substr(1));
                
              });
              
            })(jQuery);





        </script>      
